# ============================================
# PAGTRACKER - CONFIGURAÇÃO DE HOMOLOGAÇÃO
# ============================================

# Ambiente
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# Supabase (HOMOLOGAÇÃO)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# EfiPay (HOMOLOGAÇÃO)
EFIPAY_CLIENT_ID=Client_Id_your_homolog_client_id
EFIPAY_CLIENT_SECRET=Client_Secret_your_homolog_client_secret
EFIPAY_SANDBOX=true
EFIPAY_CERTIFICATE_PATH=/app/certificates/homologacao.p12

# Multi-tenancy
TENANT_CONTEXT_ENABLED=true
DEFAULT_TENANT_ID=homologacao

# Cloudflare (para SSL automático)
CLOUDFLARE_EMAIL=your-email@domain.com
CLOUDFLARE_API_KEY=your-global-api-key
CF_ZONE_ID=your-zone-id

# Redis
REDIS_PASSWORD=your-secure-redis-password
REDIS_URL=redis://redis:6379

# Webhooks
WEBHOOK_SECRET=your-webhook-secret-key
WEBHOOK_URL=https://webhooks.pagtracker.com.br

# Domínio principal
NEXT_PUBLIC_APP_URL=https://homolog.pagtracker.com.br
NEXT_PUBLIC_API_URL=https://homolog.pagtracker.com.br/api

# Logs e Monitoramento
LOG_LEVEL=info
ENABLE_METRICS=true

# Segurança
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-32-char-encryption-key

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_WEBHOOK_PER_MINUTE=1000 