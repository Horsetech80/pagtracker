{
  "name": "pagtracker-v4",
  "version": "4.0.0",
  "description": "Sistema subadquirente multi-gateway unificado - VersÃ£o Simplificada",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:production": "NODE_ENV=production npm run build",
    "start:production": "NODE_ENV=production node .next/standalone/server.js",
    "docker:build": "docker build -t pagtracker:v4 .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:production": "docker-compose -f docker-compose.production.yml up -d",
    "docker:production:down": "docker-compose -f docker-compose.production.yml down",
    "setup:env": "node scripts/setup-production-env.js",
    "setup:ssl": "node scripts/setup-ssl-domain.js",
    "deploy:production": "node scripts/deploy-production.js",
    "check:production": "node scripts/check-production-readiness.js",
    "monitor": "node scripts/monitor-services.js",
    "clean": "rm -rf .next node_modules",
    "fresh": "npm run clean && npm install",
    "backup": "mkdir -p backups && cp -r .next .env.production backups/backup-$(date +%Y%m%d-%H%M%S)/",
    "health": "curl -f http://localhost:3000/api/health || echo 'Service not available'"
  },
